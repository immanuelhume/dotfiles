{
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "workbench.iconTheme": "chalice-icon-theme",
  "workbench.editor.closeEmptyGroups": false,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/env/**": true,
    "**/venv/**": true,
    "env-*": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.fontFamily": "mononoki nerd font, monospace",
  "editor.fontSize": 15,
  "editor.fontLigatures": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": false
  },
  "editor.linkedEditing": true,
  "editor.minimap.enabled": false,
  "editor.cursorSurroundingLines": 12,
  "editor.cursorBlinking": "expand",
  "editor.tabSize": 2,
  "editor.cursorStyle": "line",
  "editor.insertSpaces": false,
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "editor.rulers": [80],
  "breadcrumbs.enabled": false,
  "editor.cursorSmoothCaretAnimation": true,
  "editor.renderLineHighlight": "gutter",
  "editor.smoothScrolling": true,
  "explorer.decorations.colors": false,
  "workbench.editor.showIcons": false,
  "workbench.editor.tabSizing": "shrink",
  "workbench.tree.indent": 10,
  "terminal.integrated.fontSize": 16,
  "vim.useCtrlKeys": false,
  "vim.scroll": 12,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "update.mode": "none",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.preferences.importModuleSpecifier": "relative",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "python.formatting.provider": "black",
  "python.formatting.autopep8Args": ["--ignore=E402,E501"],
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--max-line-length=120"],
  "python.languageServer": "Jedi",
  "autoDocstring.docstringFormat": "sphinx",
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "[svelte]": {
    "editor.defaultFormatter": "svelte.svelte-vscode"
  },
  "[prisma]": {
    "editor.defaultFormatter": "Prisma.prisma"
  },
  "[latex]": {
    "editor.formatOnPaste": false,
    "editor.suggestSelection": "recentlyUsedByPrefix"
  },
  "[go]": {
    "editor.insertSpaces": false,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": true
    },
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.defaultFormatter": "golang.go"
  },
  "workbench.startupEditor": "newUntitledFile",
  "editor.suggestSelection": "first",
  "material-ui-snippets.showNotesOnStartup": false,
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-xelatex",
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "%DOC%"
      ]
    }
  ],
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
    "editor.defaultFormatter": "Dart-Code.dart-code"
  },
  "[sql]": {
    "editor.defaultFormatter": "mtxr.sqltools"
  },
  "go.toolsManagement.autoUpdate": true,
  "workbench.colorTheme": "papercolor-vscode-light",
  "window.zoomLevel": 1,
  "python.pythonPath": "/home/junyi/.pyenv/versions/3.8.5/bin/python"
}
